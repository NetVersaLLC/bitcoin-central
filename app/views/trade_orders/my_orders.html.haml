%h2= t(".active_orders", :count => @trade_orders.active.count)
= render :partial => "trade_orders/my_orders", :locals => { :trade_orders => @trade_orders.active }

%h2= t(".inactive_orders", :count => @trade_orders.inactive.count)
= render :partial => "trade_orders/my_orders", :locals => { :trade_orders => @trade_orders.inactive }